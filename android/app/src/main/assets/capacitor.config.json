{
	"appId": "io.ionic.chainfox",
	"appName": "ChainFox",
	"webDir": "www",
	"bundledWebRuntime": false,
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"Badge": {
			"persist": true,
			"autoClear": false
		},
		"SplashScreen": {
			"launchAutoHide": false
		}
	}
}
